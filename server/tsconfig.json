{
  "display": "Node 16",

  "compilerOptions": {
    "lib": ["es2021"],
    "module": "ESNext",
    "target": "ESNext",
    "outDir": "dist",
    "rootDir": "api",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
